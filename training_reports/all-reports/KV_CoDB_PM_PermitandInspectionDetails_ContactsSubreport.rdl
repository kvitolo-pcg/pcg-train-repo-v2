        </Field>
        <Field Name="ADDRESS_LINE2">
          <DataField>ADDRESS_LINE2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ROWNBR">
          <DataField>ROWNBR</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
